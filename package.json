{
  "name": "uk.usyless.8mb",
  "version": "1.0.0",
  "description": "Local browser based video compressor",
  "homepage": "https://github.com/usyless/8mb#readme",
  "bugs": {
    "url": "https://github.com/usyless/8mb/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/usyless/8mb.git"
  },
  "license": "AGPL-3.0",
  "author": "usy",
  "type": "commonjs",
  "main": "src/index.js",
  "scripts": {
    "download": "node download-ffmpeg.js",
    "build-dev": "esbuild src/index.js --bundle --format=esm --platform=browser --outfile=src/index.out.js",
    "build": "esbuild src/index.js --bundle --format=esm --minify --platform=browser --outfile=src/index.out.js",
    "dev": "npm run build-dev && http-server src/ -p 8080 -o"
  },
  "devDependencies": {
    "esbuild": "^0.25.9",
    "http-server": "^14.1.1"
  },
  "dependencies": {
    "@ffmpeg/ffmpeg": "^0.12.15",
    "@ffmpeg/util": "^0.12.2"
  }
}
